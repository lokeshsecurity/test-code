<div class="row">
	<article class="col-sm-12 col-md-12 col-lg-12">

		<!-- Widget ID (each widget will need unique ID)-->
		<div class="jarviswidget" id="wid-id-1" data-widget-editbutton="false"
			data-widget-custombutton="false" data-widget-deletebutton="false"
			data-widget-fullscreenbutton="false">
			<!-- widget options:
					usage: <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">
					
					data-widget-colorbutton="false"	
					data-widget-editbutton="false"
					data-widget-togglebutton="false"
					data-widget-deletebutton="false"
					data-widget-fullscreenbutton="false"
					data-widget-custombutton="false"
					data-widget-collapsed="true" 
					data-widget-sortable="false"
					
				-->
			<header>
				<span class="widget-icon "> <i class="fa fa-edit"></i>
				</span>
				<h2>عنوان الوثيقه</h2>
			</header>

			<!-- widget div-->
			<div>

				<!-- widget edit box -->
				<div class="jarviswidget-editbox">
					<!-- This area used as dropdown edit box -->

				</div>
				<!-- end widget edit box -->

				<!-- widget content -->
				<div class="widget-body no-padding smart-form">

					<div class="row">


						<div class="col-md-12">
							<textarea rows="5" cols="130"></textarea>

						</div>


					</div>





				</div>
				<!-- end widget content -->

			</div>
			<!-- end widget div -->

		</div>
		<!-- end widget -->


	</article>
	<article class="col-sm-12 col-md-12 col-lg-12">

		<!-- Widget ID (each widget will need unique ID)-->
		<div class="jarviswidget" id="wid-id-28" data-widget-editbutton="false"
			data-widget-custombutton="false" data-widget-deletebutton="false"
			data-widget-fullscreenbutton="false">
			<!-- widget options:
					usage: <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">
					
					data-widget-colorbutton="false"	
					data-widget-editbutton="false"
					data-widget-togglebutton="false"
					data-widget-deletebutton="false"
					data-widget-fullscreenbutton="false"
					data-widget-custombutton="false"
					data-widget-collapsed="true" 
					data-widget-sortable="false"
					
				-->
			<header>
				<span class="widget-icon "> <i class="fa fa-edit"></i>
				</span>
				<h2>معلومات اضافيه</h2>
			</header>

			<!-- widget div-->
			<div>

				<!-- widget edit box -->
				<div class="jarviswidget-editbox">
					<!-- This area used as dropdown edit box -->

				</div>
				<!-- end widget edit box -->

				<!-- widget content -->
				<div class="widget-body no-padding smart-form">




					<div class="row">

						<section class="col col-8">
							<label class="input"><input disabled="disabled" placeholder="أسم معد الوثيقه"> </label>
						</section>
						<section class="col col-2">
							<label class="input"><input disabled="disabled" placeholder="رقم هوية المعد "></label>
						</section>
						<section class="col col-2">
							<label class="input"> اعداد الموظف</label>
						</section>


					</div>
					<div class="row">

						<section class="col col-8">
							<label class="input"><input> </label>
						</section>
						<section class="col col-2">
							<label class="input"><input></label>
						</section>
						<section class="col col-2">
							<label class="input"> المحكمه</label>
						</section>


					</div>
					<div class="row">

						<section class="col col-8">
							<label class="input"><input disabled="disabled" placeholder="اسم مسجل الوثيقه"> </label>
						</section>
						<section class="col col-2">
							<label class="input"><input disabled="disabled" placeholder="رقم هوية المسجل"></label>
						</section>
						<section class="col col-2">
							<label class="input">تسجيل الموظف</label>
						</section>


					</div>
					<div class="row">
					
					<section class="col col-8">
							<label class="input">
							</label>
						</section>

										<section class="col col-2">
											<label class="input">تاريخ الادخال</label>
										</section>
										<section class="col col-2">
												<label class="input"> <i ng-click="open($event)"
													class="icon-append fa fa-calendar"></i> <input type="text"
													class="form-control" datepicker-popup="yyyy-MM-dd"
													ng-model="todayDate" is-open="opened" ng-change="getSelectedDate()"
													datepicker-options="dateOptions"
													ng-required="true"
													close-text="Close"  gbs-date/>
												</label>
											</section>
										
								{{todayDate}}


					</div>
					
				</div>
				<!-- end widget content -->

			</div>
			<!-- end widget div -->

		</div>
		<!-- end widget -->


	</article>
</div>

